{
  "name": "@s-ui/decorators",
  "version": "1.3.0",
  "description": "> Set of ES6 decorators to improve your apps",
  "main": "lib/",
  "scripts": {
    "lib": "rm -Rf ./lib && mkdir -p ./lib && babel --presets sui ./src --out-dir ./lib",
    "prepublish": "npm run lib",
    "test": "NODE_ENV=test karma start --single-run",
    "test:ci": "NODE_ENV=test ./node_modules/.bin/karma start --single-run --browsers Firefox",
    "test:browser:watch": "NODE_ENV=test karma start --log-level disable",
    "test:server:watch": "NODE_ENV=test mocha ./test/server --compilers js:babel-core/register --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-preset-sui": "1",
    "babelify": "7.3.0",
    "browserify": "13.3.0",
    "browserify-babel-istanbul": "0.4.0",
    "chai": "3.5.0",
    "envify": "4.0.0",
    "karma": "1.4.0",
    "karma-browserify": "5.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-clear-screen-reporter": "1.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-spec-reporter": "0.0.26",
    "mocha": "3.2.0",
    "sinon": "1.17.7",
    "watchify": "3.8.0"
  },
  "dependencies": {
    "guild": "1.2.2",
    "lru-cache": "4.0.2"
  },
  "browser": {
    "http": false,
    "https": false
  }
}
